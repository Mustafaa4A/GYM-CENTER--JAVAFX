<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="478.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oodj_assignment.SessionViewController">
    <TableView fx:id="tbSession" layoutX="7.0" layoutY="83.0" prefHeight="389.0" prefWidth="689.0">
      <columns>
          <TableColumn fx:id="tcId" prefWidth="64.0" text="ID" />
          <TableColumn fx:id="tcName" prefWidth="248.0" text="Trainer Name" />
          <TableColumn fx:id="tcStart" prefWidth="118.0" text="Start Time" />
          <TableColumn fx:id="tcEnd" prefWidth="99.0" text="End Time" />
          <TableColumn fx:id="tcFee" prefWidth="153.0" text="Fee" />
      </columns>
    </TableView>
   <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="20.0" prefWidth="20.0">
      <graphic>
         <ImageView fitHeight="43.0" fitWidth="101.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@img/back.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
</AnchorPane>
