<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="483.0" prefWidth="688.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.oodj_assignment.register_trainingController">
   <Label layoutX="219.0" layoutY="27.0" text="Register Training Session">
      <font>
         <Font name="System Bold" size="24.0"/>
      </font>
   </Label>
   <Label layoutX="48.0" layoutY="155.0" text="Trainer Name">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <Label layoutX="50.0" layoutY="197.0" text="Start time">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <TextField fx:id="txtStart" layoutX="174.0" layoutY="188.0" prefHeight="30.0" prefWidth="431.0">
      <font>
         <Font size="14.0"/>
      </font>
   </TextField>
   <Label layoutX="48.0" layoutY="237.0" text="End time">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <TextField fx:id="txtEnd" layoutX="174.0" layoutY="231.0" prefHeight="30.0" prefWidth="431.0">
      <font>
         <Font size="14.0"/>
      </font>
   </TextField>
   <Button fx:id="btnRegister" layoutX="292.0" layoutY="356.0" mnemonicParsing="false" onAction="#register"
           prefHeight="20.0" prefWidth="117.0" text="Register">
      <font>
         <Font size="15.0"/>
      </font>
   </Button>
   <Label fx:id="lblMessage" layoutX="59.0" layoutY="91.0" prefHeight="20.0" prefWidth="552.0"
          style="-fx-alignment: center;" textFill="#d31818">
      <font>
         <Font size="20.0"/>
      </font>
   </Label>
   <Label layoutX="48.0" layoutY="286.0" text="Fee">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <TextField fx:id="txtFee" layoutX="174.0" layoutY="282.0" prefHeight="30.0" prefWidth="431.0">
      <font>
         <Font size="14.0"/>
      </font>
   </TextField>
   <MenuButton fx:id="mbTrainer" layoutX="174.0" layoutY="140.0" mnemonicParsing="false" onMouseEntered="#readTrainer"
               prefHeight="31.0" prefWidth="431.0" text="Choose a trainer"/>
   <Button fx:id="btnBack" layoutX="3.0" layoutY="11.0" mnemonicParsing="false" onAction="#back" prefHeight="20.0"
           prefWidth="20.0">
      <graphic>
         <ImageView fitHeight="43.0" fitWidth="101.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@img/back.png"/>
            </image>
         </ImageView>
      </graphic>
   </Button>
</AnchorPane>
